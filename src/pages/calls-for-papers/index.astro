import CallForPapersItem from "@components/pages/calls-for-papers/CallForPapersItem.astro";
import { EXAMPLE_CALLS } from "./ExampleCall";
---

<BaseInformationPageLayout
	head={{
		title: `Calls for papers`,
		description: "Acta Mechanica et Imperium all issues",
