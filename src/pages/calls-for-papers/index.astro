>
	<div class="container mx-auto mt-8">
		<div class="mt-8 flex flex-col gap-8">
			<CallForPapersItem {...EXAMPLE_CALLS[0]} />
			<CallForPapersItem {...EXAMPLE_CALLS[0]} />
		</div>
	</div>
</BaseInformationPageLayout>
